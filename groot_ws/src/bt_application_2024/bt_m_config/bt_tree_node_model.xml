<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4">
  <BehaviorTree ID="first_test">
    <Sequence>
      <Navigation result="{GOAL1}"
                  goal="0,0,0"/>
      <Navigation result="{GOAL2}"
                  goal="1,0,0"/>
      <Navigation result="{GOAL3}"
                  goal="1,1,0"/>
    </Sequence>
  </BehaviorTree>

  <!-- Description of Node Models (used by Groot) -->
  <TreeNodesModel>
    <Action ID="Navigation">
      <output_port name="result"
                   type="geometry_msgs::TwistStamped_&lt;std::allocator&lt;void&gt; &gt;"/>
      <input_port name="goal"
                  type="geometry_msgs::TwistStamped_&lt;std::allocator&lt;void&gt; &gt;"/>
    </Action>
  </TreeNodesModel>

</root>
