<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4">
  <BehaviorTree ID="BannerMission">
    <Sequence>
      <Navigation final_pose=""
                  type=""
                  goal=""/>
      <Docking mission_type=""
               final_pose=""
               offset=""
               base=""/>
      <Sleep msec=""/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="MainTree">
    <Sequence>
      <ForceSuccess>
        <SubTree ID="MissionPointOne"
                 _autoremap="true"/>
      </ForceSuccess>
      <ForceSuccess>
        <SubTree ID="MissionPointThree"
                 _autoremap="true"/>
      </ForceSuccess>
      <ForceSuccess>
        <SubTree ID="MissionPointFour"
                 _autoremap="true"/>
      </ForceSuccess>
      <Navigation final_pose="{PrevGoal}"
                  type="0"
                  goal="800, 1700, 0"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="MissionPointFour">
    <Sequence>
      <Navigation final_pose="{PrevGoal}"
                  type="1"
                  goal="0.8, 0.5, 0"/>
      <Docking mission_type="0"
               final_pose="{PrevGoal}"
               offset="0, -0.1, 0"
               base="{PrevGoal}"/>
      <Sleep msec="1000"/>
      <Navigation final_pose="{PrevGoal}"
                  type="1"
                  goal="0.8, 0.3, 0"/>
      <Docking mission_type="0"
               final_pose="{PrevGoal}"
               offset="0, -0.1, 0"
               base="{PrevGoal}"/>
      <PointProvider point_out2="{DISTANCE1}"
                     point_out1="{DISTANCE}"
                     point_in="0, -0.1, 0"/>
      <SubTree ID="PutTwoLevels"
               DISTANCE="{DISTANCE}"
               DISTANCE1="{DISTANCE1}"
               PrevGoal="{PrevGoal}"
               _autoremap="true"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="MissionPointOne">
    <Sequence>
      <Navigation final_pose="{PrevGoal}"
                  type="1"
                  goal="2.6, 1.3, 0"/>
      <Docking mission_type="0"
               final_pose="{PrevGoal}"
               offset="0.1, 0, 0"
               base="{PrevGoal}"/>
      <Sleep msec="1000"/>
      <Navigation final_pose="{PrevGoal}"
                  type="1"
                  goal="2.6, 0.35, 0"/>
      <Docking mission_type="0"
               final_pose="{PrevGoal}"
               offset="0.1, 0, 0"
               base="{PrevGoal}"/>
      <PointProvider point_out2="{DISTANCE1}"
                     point_out1="{DISTANCE}"
                     point_in="0.1, 0, 0"/>
      <SubTree ID="TakeOneLevel"
               DISTANCE="{DISTANCE}"
               DISTANCE1="{DISTANCE1}"
               PrevGoal="{PrevGoal}"
               _autoremap="true"/>
      <Navigation final_pose="{PrevGoal}"
                  type="1"
                  goal="2.6, 0.9, 0"/>
      <Docking mission_type="0"
               final_pose="{PrevGoal}"
               offset="0.1, 0, 0"
               base="{PrevGoal}"/>
      <SubTree ID="PutThreeLevels"
               DISTANCE="{DISTANCE}"
               DISTANCE1="{DISTANCE1}"
               PrevGoal="{PrevGoal}"
               _autoremap="true"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="MissionPointThree">
    <Sequence>
      <Navigation final_pose="{PrevGoal}"
                  type="1"
                  goal="2, 0.9, 0"/>
      <Docking mission_type="0"
               final_pose="{PrevGoal}"
               offset="-0.1, 0, 0"
               base="{PrevGoal}"/>
      <Sleep msec="1000"/>
      <Navigation final_pose="{PrevGoal}"
                  type="1"
                  goal="2.3, 0.5, 0"/>
      <Docking mission_type="0"
               final_pose="{PrevGoal}"
               offset="0, -0.1, 0"
               base="{PrevGoal}"/>
      <PointProvider point_out2="{DISTANCE1}"
                     point_out1="{DISTANCE}"
                     point_in="0, -0.1, 0"/>
      <SubTree ID="TakeOneLevel"
               DISTANCE="{DISTANCE}"
               DISTANCE1="{DISTANCE1}"
               PrevGoal="{PrevGoal}"
               _autoremap="true"/>
      <Navigation final_pose="{PrevGoal}"
                  type="1"
                  goal="1.25, 0.3, 0"/>
      <Docking mission_type="0"
               final_pose="{PrevGoal}"
               offset="0, -0.1, 0"
               base="{PrevGoal}"/>
      <SubTree ID="PutThreeLevels"
               DISTANCE="{DISTANCE}"
               DISTANCE1="{DISTANCE1}"
               PrevGoal="{PrevGoal}"
               _autoremap="true"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="NavTest">
    <Sequence>
      <Navigation final_pose="{PrevGoal}"
                  type="0"
                  goal="2.3, 1.3, 0"/>
      <Sleep msec="1000"/>
      <Docking mission_type="0"
               final_pose="{PrevGoal}"
               offset="0.1, 0, 0"
               base="{PrevGoal}"/>
      <Sleep msec="1000"/>
      <Docking mission_type="0"
               final_pose="{PrevGoal}"
               offset="-0.1, 0, 0"
               base="{PrevGoal}"/>
      <Navigation final_pose="{PrevGoal}"
                  type="0"
                  goal="2.3, 0.4, 0"/>
      <Docking mission_type="0"
               final_pose="{PrevGoal}"
               offset="-0.1, 0, 0"
               base="{PrevGoal}"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="PutThreeLevels">
    <Sequence>
      <Sleep msec="1000"/>
      <Docking mission_type="0"
               final_pose="{PrevGoal}"
               offset="{DISTANCE1}"
               base="{PrevGoal}"/>
      <Navigation final_pose="{PrevGoal}"
                  type="0"
                  goal="0, 0, 3.14"/>
      <SubTree ID="PutTwoLevels"
               DISTANCE="{DISTANCE}"
               DISTANCE1="{DISTANCE1}"
               _autoremap="true"/>
      <Docking mission_type="0"
               final_pose="{PrevGoal}"
               offset="{DISTANCE1}"
               base="{PrevGoal}"/>
      <Sleep msec="1000"/>
      <Docking mission_type="0"
               final_pose="{PrevGoal}"
               offset="{DISTANCE}"
               base="{PrevGoal}"/>
      <Sleep msec="1000"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="PutTwoLevels">
    <Sequence>
      <Sleep msec="1000"/>
      <Docking mission_type="0"
               final_pose="{PrevGoal}"
               offset="{DISTANCE}"
               base="{PrevGoal}"/>
      <Navigation final_pose="{PrevGoal}"
                  type="0"
                  goal="0, 0, 3.14"/>
      <Docking mission_type="0"
               final_pose="{PrevGoal}"
               offset="{DISTANCE1}"
               base="{PrevGoal}"/>
      <SubTree ID="TakeOneLevel"
               DISTANCE="{DISTANCE}"
               DISTANCE1="{DISTANCE1}"
               _autoremap="true"/>
      <Docking mission_type="0"
               final_pose="{PrevGoal}"
               offset="{DISTANCE}"
               base="{PrevGoal}"/>
      <Sleep msec="1000"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="TakeOneLevel">
    <Sequence>
      <Sleep msec="1000"/>
      <Docking mission_type="0"
               final_pose="{PrevGoal}"
               offset="{DISTANCE1}"
               base="{PrevGoal}"/>
      <Sleep msec="1000"/>
      <Docking mission_type="0"
               final_pose="{PrevGoal}"
               offset="{DISTANCE}"
               base="{PrevGoal}"/>
      <Sleep msec="1000"/>
      <Docking mission_type="0"
               final_pose="{PrevGoal}"
               offset="{DISTANCE1}"
               base="{PrevGoal}"/>
      <Sleep msec="1000"/>
    </Sequence>
  </BehaviorTree>

  <!-- Description of Node Models (used by Groot) -->
  <TreeNodesModel>
    <Action ID="Docking">
      <input_port name="mission_type"
                  type="int"/>
      <output_port name="final_pose"
                   type="geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt;"/>
      <input_port name="offset"
                  type="geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt;"/>
      <input_port name="base"
                  type="geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt;"/>
    </Action>
    <Action ID="Navigation">
      <output_port name="final_pose"
                   type="geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt;"/>
      <input_port name="type"
                  type="int"/>
      <input_port name="goal"
                  type="geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt;"/>
    </Action>
    <Action ID="PointProvider">
      <output_port name="point_out2"
                   type="geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt;"/>
      <output_port name="point_out1"
                   type="geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt;"/>
      <input_port name="point_in"
                  type="geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt;"/>
    </Action>
  </TreeNodesModel>

</root>
