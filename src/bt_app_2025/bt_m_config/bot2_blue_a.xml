<?xml version='1.0' encoding='utf-8'?>
<root BTCPP_format="4">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <Parallel failure_count="2" success_count="1">
        <Sequence>
          <Sequence>
            <Sequence>
              <BTStarter result="{STATUS}" />
            </Sequence>
            <ForceSuccess>
              <Timeout msec="6000">
                <FirmwareMission mission_status="{x}" mission_type="31" />
              </Timeout>
            </ForceSuccess>
            <Sleep msec="1000" />
            <Parallel failure_count="2" success_count="1">
              <Docking final_pose="{PrevGoal}" isPureDocking="0" dock_type="dock_x_delaySpin" shift="0" offset="0" base="1.8, 0.5, 1" />
              <Delay delay_msec="1000">
                <ForceSuccess>
                  <Timeout msec="6000">
                    <FirmwareMission mission_status="{x}" mission_type="100" />
                  </Timeout>
                </ForceSuccess>
              </Delay>
            </Parallel>
            <SubTree ID="MissionPointOne" _autoremap="true" />
            <SubTree ID="MissionPointThree" _autoremap="true" />
          </Sequence>
          <Repeat num_cycles="100">
            <Sequence>
              <RetryUntilSuccessful num_attempts="2">
                <Docking final_pose="{PrevGoal}" isPureDocking="0" dock_type="dock_x" shift="0" offset="0" base="2.0, 1.0, 2" />
              </RetryUntilSuccessful>
              <RetryUntilSuccessful num_attempts="2">
                <Docking final_pose="{PrevGoal}" isPureDocking="0" dock_type="dock_x" shift="0" offset="0" base="2.0, 1.0, 1" />
              </RetryUntilSuccessful>
              <RetryUntilSuccessful num_attempts="2">
                <Docking final_pose="{PrevGoal}" isPureDocking="0" dock_type="dock_x" shift="0" offset="0" base="2.0, 1.0, 0" />
              </RetryUntilSuccessful>
              <RetryUntilSuccessful num_attempts="2">
                <Docking final_pose="{PrevGoal}" isPureDocking="0" dock_type="dock_x" shift="0" offset="0" base="2.0, 1.0, 3" />
              </RetryUntilSuccessful>
            </Sequence>
          </Repeat>
        </Sequence>
        <Sleep msec="90000" />
      </Parallel>
      <Parallel failure_count="2" success_count="1">
        <Sequence>
          <ForceSuccess>
            <Timeout msec="5000">
              <FirmwareMission mission_status="{x}" mission_type="1020" />
            </Timeout>
          </ForceSuccess>
          <ForceSuccess>
            <Timeout msec="5000">
              <FirmwareMission mission_status="{x}" mission_type="101" />
            </Timeout>
          </ForceSuccess>
        </Sequence>
        <Docking final_pose="{PrevGoal}" isPureDocking="0" dock_type="dock_x" shift="0" offset="0" base="2.6, 1.5, 1" />
      </Parallel>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="MissionPointOne">
    <Sequence>
      <RetryUntilSuccessful num_attempts="2">
        <Docking final_pose="{PrevGoal}" isPureDocking="0" dock_type="dock_y_precise_gentle" shift="0" offset="0.18" base="1.9, 0.75, 1" />
      </RetryUntilSuccessful>
      <Parallel failure_count="2" success_count="1">
        <RetryUntilSuccessful num_attempts="2">
          <Docking final_pose="{PrevGoal}" isPureDocking="1" dock_type="dock_y_precise_gentle_slow" shift="0" offset="0.1" base="{PrevGoal}" />
        </RetryUntilSuccessful>
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="16" />
          </Timeout>
        </ForceSuccess>
      </Parallel>
      <RetryUntilSuccessful num_attempts="2">
        <Docking final_pose="{PrevGoal}" isPureDocking="0" dock_type="dock_y_precise_gentle" shift="0" offset="-0.18" base="2.22, 0.5, 1" />
      </RetryUntilSuccessful>
      <Parallel failure_count="2" success_count="1">
        <RetryUntilSuccessful num_attempts="2">
          <Docking final_pose="{PrevGoal}" isPureDocking="1" dock_type="dock_y_precise_gentle_slow" shift="0" offset="-0.1" base="{PrevGoal}" />
        </RetryUntilSuccessful>
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="26" />
          </Timeout>
        </ForceSuccess>
      </Parallel>
      <RetryUntilSuccessful num_attempts="2">
        <Docking final_pose="{PrevGoal}" isPureDocking="0" dock_type="dock_x" shift="0" offset="0" base="1.78, 0.29, 2" />
      </RetryUntilSuccessful>
      <ForceSuccess>
        <Timeout msec="6000">
          <FirmwareMission mission_status="{x}" mission_type="52" />
        </Timeout>
      </ForceSuccess>
      <RetryUntilSuccessful num_attempts="2">
        <Docking final_pose="{PrevGoal}" isPureDocking="1" dock_type="dock_x" shift="0" offset="0.2" base="{PrevGoal}" />
      </RetryUntilSuccessful>
      <RetryUntilSuccessful num_attempts="2">
        <Docking final_pose="{PrevGoal}" isPureDocking="0" dock_type="dock_x" shift="0" offset="0" base="1.98, 0.29, 0" />
      </RetryUntilSuccessful>
      <ForceSuccess>
        <Timeout msec="6000">
          <FirmwareMission mission_status="{x}" mission_type="62" />
        </Timeout>
      </ForceSuccess>
      <RetryUntilSuccessful num_attempts="2">
        <Docking final_pose="{PrevGoal}" isPureDocking="1" dock_type="dock_x" shift="0" offset="0.2" base="{PrevGoal}" />
      </RetryUntilSuccessful>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="MissionPointThree">
    <Sequence>
      <RetryUntilSuccessful num_attempts="2">
        <Docking final_pose="{PrevGoal}" isPureDocking="0" dock_type="dock_y_precise_gentle" shift="0" offset="0" base="2.166, 1.55, 1" />
      </RetryUntilSuccessful>
      <Parallel failure_count="2" success_count="1">
        <RetryUntilSuccessful num_attempts="2">
          <Docking final_pose="{PrevGoal}" isPureDocking="1" dock_type="dock_y_precise_gentle_slow" shift="0" offset="0.08" base="{PrevGoal}" />
        </RetryUntilSuccessful>
        <Sleep msec="700" />
      </Parallel>
      <Parallel failure_count="2" success_count="1">
        <RetryUntilSuccessful num_attempts="2">
          <Docking final_pose="{PrevGoal}" isPureDocking="1" dock_type="dock_y_precise_gentle_slow" shift="0" offset="0.04" base="{PrevGoal}" />
        </RetryUntilSuccessful>
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="16" />
          </Timeout>
        </ForceSuccess>
      </Parallel>
      <RetryUntilSuccessful num_attempts="2">
        <Docking final_pose="{PrevGoal}" isPureDocking="0" dock_type="dock_y" shift="0" offset="0" base="2.22, 0.26, 3" />
      </RetryUntilSuccessful>
      <ForceSuccess>
        <Timeout msec="6000">
          <FirmwareMission mission_status="{x}" mission_type="52" />
        </Timeout>
      </ForceSuccess>
      <RetryUntilSuccessful num_attempts="2">
        <Docking final_pose="{PrevGoal}" isPureDocking="1" dock_type="dock_y" shift="0" offset="0.2" base="{PrevGoal}" />
      </RetryUntilSuccessful>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="MissionPointTwo">
    <Sequence>
      <RetryUntilSuccessful num_attempts="2">
        <Docking final_pose="{PrevGoal}" isPureDocking="0" dock_type="dock_x_precise" shift="0" offset="0" base="2.74, 0.413, 0" />
      </RetryUntilSuccessful>
      <Parallel failure_count="2" success_count="1">
        <RetryUntilSuccessful num_attempts="2">
          <Docking final_pose="{PrevGoal}" isPureDocking="1" dock_type="dock_x_precise_gentle_slow" shift="0" offset="0.1" base="{PrevGoal}" />
        </RetryUntilSuccessful>
        <Sleep msec="700" />
      </Parallel>
      <Parallel failure_count="2" success_count="1">
        <RetryUntilSuccessful num_attempts="2">
          <Docking final_pose="{PrevGoal}" isPureDocking="1" dock_type="dock_x_precise_gentle_slow" shift="0" offset="0.1" base="{PrevGoal}" />
        </RetryUntilSuccessful>
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="16" />
          </Timeout>
        </ForceSuccess>
      </Parallel>
      <Parallel failure_count="2" success_count="1">
        <Delay delay_msec="1000">
          <ForceSuccess>
            <Timeout msec="6000">
              <FirmwareMission mission_status="{x}" mission_type="13" />
            </Timeout>
          </ForceSuccess>
        </Delay>
        <RetryUntilSuccessful num_attempts="2">
          <Docking final_pose="{PrevGoal}" isPureDocking="0" dock_type="dock_x_precise_gentle_slow_delaySpin" shift="0" offset="0" base="2.74, 1.33, 2" />
        </RetryUntilSuccessful>
      </Parallel>
      <Parallel failure_count="2" success_count="1">
        <RetryUntilSuccessful num_attempts="2">
          <Docking final_pose="{PrevGoal}" isPureDocking="1" dock_type="dock_x_precise_gentle_slow" shift="0" offset="0.1" base="{PrevGoal}" />
        </RetryUntilSuccessful>
        <Sleep msec="700" />
      </Parallel>
      <Parallel failure_count="2" success_count="1">
        <RetryUntilSuccessful num_attempts="2">
          <Docking final_pose="{PrevGoal}" isPureDocking="1" dock_type="dock_x_precise_gentle_slow" shift="0" offset="0.1" base="{PrevGoal}" />
        </RetryUntilSuccessful>
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="26" />
          </Timeout>
        </ForceSuccess>
      </Parallel>
      <RetryUntilSuccessful num_attempts="2">
        <Docking final_pose="{PrevGoal}" isPureDocking="0" dock_type="dock_x" shift="0" offset="0" base="2.13, 0.29, 2" />
      </RetryUntilSuccessful>
      <Delay delay_msec="500">
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="12" />
          </Timeout>
        </ForceSuccess>
      </Delay>
      <RetryUntilSuccessful num_attempts="2">
        <Docking final_pose="{PrevGoal}" isPureDocking="0" dock_type="dock_x_precise_gentle_slow" shift="0" offset="0" base="2.25, 0.315, 2" />
      </RetryUntilSuccessful>
      <ForceSuccess>
        <Timeout msec="6000">
          <FirmwareMission mission_status="{x}" mission_type="14" />
        </Timeout>
      </ForceSuccess>
      <Sleep msec="1000" />
      <RetryUntilSuccessful num_attempts="2">
        <Docking final_pose="{PrevGoal}" isPureDocking="1" dock_type="dock_x_precise_gentle_slow" shift="0" offset="-0.2" base="{PrevGoal}" />
      </RetryUntilSuccessful>
      <ForceSuccess>
        <Timeout msec="6000">
          <FirmwareMission mission_status="{x}" mission_type="15" />
        </Timeout>
      </ForceSuccess>
      <Sleep msec="1000" />
      <RetryUntilSuccessful num_attempts="2">
        <Docking final_pose="{PrevGoal}" isPureDocking="0" dock_type="dock_x" shift="0" offset="0" base="1.88, 0.29, 2" />
      </RetryUntilSuccessful>
      <ForceSuccess>
        <Timeout msec="6000">
          <FirmwareMission mission_status="{x}" mission_type="12" />
        </Timeout>
      </ForceSuccess>
      <RetryUntilSuccessful num_attempts="2">
        <Docking final_pose="{PrevGoal}" isPureDocking="0" dock_type="dock_x" shift="0" offset="0" base="2.35, 0.29, 2" />
      </RetryUntilSuccessful>
      <RetryUntilSuccessful num_attempts="2">
        <Docking final_pose="{PrevGoal}" isPureDocking="0" dock_type="dock_x_fast" shift="0" offset="0" base="2.35, 0.29, 0" />
      </RetryUntilSuccessful>
      <RetryUntilSuccessful num_attempts="2">
        <Docking final_pose="{PrevGoal}" isPureDocking="0" dock_type="dock_x_slow" shift="0" offset="0" base="2.03, 0.29, 0" />
      </RetryUntilSuccessful>
      <ForceSuccess>
        <Timeout msec="6000">
          <FirmwareMission mission_status="{x}" mission_type="27" />
        </Timeout>
      </ForceSuccess>
      <RetryUntilSuccessful num_attempts="2">
        <Docking final_pose="{PrevGoal}" isPureDocking="1" dock_type="dock_x" shift="0" offset="0.2" base="{PrevGoal}" />
      </RetryUntilSuccessful>
    </Sequence>
  </BehaviorTree>

  
  <TreeNodesModel>
    <Action ID="BTStarter">
      <output_port name="result" type="int" />
    </Action>
    <Action ID="Docking">
      <output_port name="final_pose" type="geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt;" />
      <input_port name="isPureDocking" type="bool" />
      <input_port name="dock_type" type="std::string" />
      <input_port name="shift" type="double" />
      <input_port name="offset" type="double" />
      <input_port name="base" type="geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt;" />
    </Action>
    <Action ID="FirmwareMission">
      <output_port name="mission_status" type="int" />
      <input_port name="mission_type" type="int" />
    </Action>
  </TreeNodesModel>

</root>