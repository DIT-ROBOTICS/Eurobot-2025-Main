<?xml version='1.0' encoding='utf-8'?>
<root>
  <BehaviorTree ID="DockingSubTree">
    <Sequence>
      <MySetBlackboard new_value="{Timeout}" blackboard_value="false" blackboard_key="Timeout" />
      <RetryUntilSuccessful num_attempts="{retry}" _failureIf="Timeout">
        <Fallback>
          <Parallel failure_count="1" success_count="1">
            <Docking final_pose="{final_pose}" isPureDocking="{isPureDocking}" dock_type="{dock_type}" shift="{shift}" offset="{offset}" base="{base}" _description="step back" />
            <Sequence>
              <Sleep msec="{timeout}" />
              <GetLocation locaization_target="robot" pose="{PrevGoal}"/>
              <MySetBlackboard new_value="{Timeout}" blackboard_value="true" blackboard_key="Timeout" />
            </Sequence>
          </Parallel>
          <ForceFailure _successIf="Timeout">
            <Sleep msec="{sleep}" />
          </ForceFailure>
        </Fallback>
      </RetryUntilSuccessful>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="MainTree">
    <Sequence>
      <ForceSuccess>
        <Timeout msec="100">
          <FirmwareMission mission_status="{x}" mission_type="1" _description="arm up" />
        </Timeout>
      </ForceSuccess>
      <ForceSuccess>
        <Timeout msec="4000">
          <FirmwareMission mission_status="{x}" mission_type="81" _description="back arm take raw material" />
        </Timeout>
      </ForceSuccess>
      <Sleep msec="1000"/>
      <ForceSuccess>
        <Timeout msec="4000">
          <FirmwareMission mission_status="{x}" mission_type="80" _description="back arm take raw material" />
        </Timeout>
      </ForceSuccess>
      <Parallel failure_count="2" success_count="1">
        <SubTree ID="Approval" _autoremap="true" />
        <Sleep msec="95000" />
      </Parallel>
      <ForceSuccess>
        <Timeout msec="6000">
          <FirmwareMission mission_status="{x}" mission_type="0" _description="backward stretch out" />
        </Timeout>
      </ForceSuccess>
      <GetLocation locaization_target="robot" pose="{PrevGoal}"/>
      <Rotation final_pose="{PrevGoal}" degree="30" base="{PrevGoal}"/>
      <Docking final_pose="{PrevGoal}" isPureDocking="0" dock_type="dock_y_slow" shift="0" offset="0" base="2.55, 1.5, 0.5" />
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="Approval">
    <Sequence>
      <SubTree ID="StartSubTree" _autoremap="true" />
      <GetLocation locaization_target="robot" pose="{PrevGoal}"/>
      <SubTree ID="ApprovalSubTree" material_index="6" mission_index="13" deg1="0" deg2="0" stage_point1="1.1, 0.7, 1" stage_point2="1.3, 0.4, 3" _autoremap="true" />
      <SubTree ID="ApprovalSubTree" material_index="5" mission_index="13" deg1="90" deg2="270" stage_point1="0.8, 0.6, 2" stage_point2="1.3, 0.4, 0" _autoremap="true" />
      <SubTree ID="ApprovalSubTree" material_index="7" mission_index="14" deg1="90" deg2="0" stage_point1="0.5, 0.4, 2" stage_point2="0.7, 0.4, 0" _autoremap="true" />
      <!-- <SubTree ID="ApprovalSubTree" material_index="1" mission_index="17" deg1="135" deg2="225" stage_point1="2.673, 1.33, 0.7" stage_point2="1.72, 0.3, 2.5" _autoremap="true" /> -->
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="OneLevelBot1new">
    <Fallback>
      <Sequence _failureIf="last_mission_failed">
        <ForceSuccess>
          <Timeout msec="4000">
            <FirmwareMission mission_status="{x}" mission_type="21" _description="back arm take raw material" />
          </Timeout>
        </ForceSuccess>
        <SetBlackboard value="1" output_key="hasCompleteMaterial" />
      </Sequence>
      <ForceSuccess>
        <Timeout msec="6000">
          <FirmwareMission mission_status="{x}" mission_type="20" _description="arm up" />
        </Timeout>
      </ForceSuccess>
    </Fallback>
  </BehaviorTree>

  <BehaviorTree ID="PlaceOneLevel">
    <Fallback>
      <Switch2 case_1="1" case_2="0" variable="{hasCompleteMaterial}">
        <Sequence>
          <ForceSuccess>
            <Timeout msec="6000">
              <FirmwareMission mission_status="{x}" mission_type="20" _description="back arm release 1 level" />
            </Timeout>
          </ForceSuccess>
          <ForceSuccess>
            <Timeout msec="100">
              <FirmwareMission mission_status="{x}" mission_type="25" _description="close back side arm" />
            </Timeout>
          </ForceSuccess>
          <SetBlackboard value="0" output_key="hasCompleteMaterial" />
          <MissionSuccess levels="1" base_index="{index}" />
        </Sequence>
        <Sequence>
          <ForceSuccess>
            <Timeout msec="6000">
              <FirmwareMission mission_status="{x}" mission_type="24" _description="back arm release 1 level" />
            </Timeout>
          </ForceSuccess>
          <Sleep msec="200" />
          <ForceSuccess>
            <Timeout msec="6000">
              <FirmwareMission mission_status="{x}" mission_type="23" _description="back arm release 1 level" />
            </Timeout>
          </ForceSuccess>
          <SetBlackboard value="0" output_key="hasCompleteMaterial" />
          <MissionSuccess levels="1" base_index="{index}" />
        </Sequence>
        <AlwaysFailure />
      </Switch2>
      <Sequence>
        <MySetBlackboard new_value="{last_mission_failed}" blackboard_value="true" blackboard_key="last_mission_failed" />
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="10" _description="smd strenght" />
          </Timeout>
        </ForceSuccess>
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="33" _description="smd strenght" />
          </Timeout>
        </ForceSuccess>
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="20" _description="smd strenght" />
          </Timeout>
        </ForceSuccess>
        <ForceSuccess>
          <MissionFailure failed_levels="{a}" success_levels="{b}" base_index="{index}" robot_type="1" step_results="3" />
        </ForceSuccess>
      </Sequence>
    </Fallback>
  </BehaviorTree>

  <BehaviorTree ID="PlaceTwoLevels">
    <Fallback>
      <Sequence>
        <MissionStart BACK_IN="{back}" FORWARD_IN="{forward}" SHIFT_IN="{shift}" INDEX_IN="{index}" BACK_L="{BACK_L}" BACK_M="{BACK_M}" BACK_S="{BACK_S}" FORWARD_L="{FORWARD_L}" FORWARD_M="{FORWARD_M}" FORWARD_S="{FORWARD_S}" SHIFT="{SHIFT}" DOCK_DIR="{DOCK_DIR}" />
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="35" _description="shrink little arm" />
          </Timeout>
        </ForceSuccess>
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="30" _description="arm down" />
          </Timeout>
        </ForceSuccess>
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="10" _description="front releace materials" />
          </Timeout>
        </ForceSuccess>
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="10" _description="front releace materials" />
          </Timeout>
        </ForceSuccess>
        <SubTree ID="DockingSubTree" base="{PrevGoal}" dock_type="{DOCK_DIR}" final_pose="{PrevGoal}" isPureDocking="1" offset="{BACK_M}" retry="100" shift="0" sleep="100" timeout="6000" _autoremap="true" />
        <MissionSuccess levels="2" base_index="{index}" />
      </Sequence>
      <Sequence>
        <MySetBlackboard new_value="{last_mission_failed}" blackboard_value="true" blackboard_key="last_mission_failed" />
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="10" _description="smd strenght" />
          </Timeout>
        </ForceSuccess>
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="33" _description="smd strenght" />
          </Timeout>
        </ForceSuccess>
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="20" _description="smd strenght" />
          </Timeout>
        </ForceSuccess>
        <ForceSuccess>
          <MissionFailure failed_levels="{a}" success_levels="{b}" base_index="{index}" robot_type="1" step_results="4" />
        </ForceSuccess>
      </Sequence>
    </Fallback>
  </BehaviorTree>

  <BehaviorTree ID="StartSubTree">
    <Sequence>
      <BTStarter result="{STATUS}" />
      <CamReceiver />
      <ForceSuccess>
        <Timeout msec="6000">
          <FirmwareMission mission_status="{x}" mission_type="1" _description="" />
        </Timeout>
      </ForceSuccess>
      <ForceSuccess>
        <Timeout msec="6000">
          <FirmwareMission mission_status="{x}" mission_type="90" _description="" />
        </Timeout>
      </ForceSuccess>
      <SetBlackboard value="0" output_key="hasCompleteMaterial" />
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="ApprovalSubTree">
    <ForceSuccess>
      <Sequence>
        <Rotation final_pose="{PrevGoal}" degree="{deg1}" base="{PrevGoal}"/>
        <RetryUntilSuccessful  num_attempts="100">
          <Fallback>
            <Docking final_pose="{PrevGoal}" isPureDocking="0" dock_type="dock_y_slow_precise" shift="0" offset="0" base="{stage_point1}" />
            <ForceFailure>
              <Sleep msec="1000"/>
            </ForceFailure>
          </Fallback>
        </RetryUntilSuccessful>
        <MaterialChecker remap_offset="{offset}" remap_shift="{shift}" remap_dock_type="{dock_type}" remap_base="{base}" remap_index="{index}" shift="0.045" offset="0.2" mission_type="back" dock_type="precise_slow" base_index="{material_index}">
        <RetryUntilSuccessful  num_attempts="100">
          <Fallback>
            <Docking final_pose="{PrevGoal}" isPureDocking="0" dock_type="{dock_type}" shift="{shift}" offset="{offset}" base="{base}" />
            <ForceFailure>
              <Sleep msec="1000"/>
            </ForceFailure>
          </Fallback>
        </RetryUntilSuccessful>
        </MaterialChecker>
        <SubTree ID="OneLevelBot1new" hasCompleteMaterial="{hasCompleteMaterial}" _autoremap="true" />
        <MissionSuccess levels="1" base_index="{index}" _skipIf="last_mission_failed" />
        <Rotation final_pose="{PrevGoal}" degree="{deg2}" base="{PrevGoal}"/>
        <RetryUntilSuccessful  num_attempts="100">
          <Fallback>
            <Docking final_pose="{PrevGoal}" isPureDocking="0" dock_type="dock_y_slow_precise" shift="0" offset="0" base="{stage_point2}" />
            <ForceFailure>
              <Sleep msec="1000"/>
            </ForceFailure>
          </Fallback>
        </RetryUntilSuccessful>
        <Sequence>
          <MissionChecker level="2" remap_base="{base}" mission_type="back" base_index="{mission_index}">
            <RetryUntilSuccessful  num_attempts="100">
              <Fallback>
                <Docking final_pose="{PrevGoal}" isPureDocking="0" dock_type="dock_y_slow_precise" shift="0" offset="0" base="{base}" />
                <ForceFailure>
                  <Sleep msec="1000"/>
                </ForceFailure>
              </Fallback>
            </RetryUntilSuccessful>
          </MissionChecker>
          <SubTree ID="PlaceOneLevel" index="{mission_index}" hasCompleteMaterial="{hasCompleteMaterial}" _autoremap="true" />
        </Sequence>
      </Sequence>
    </ForceSuccess>
  </BehaviorTree>

  <BehaviorTree ID="ThreeLevelsBot1new">
    <Fallback>
      <Sequence>
        <MissionStart BACK_IN="{back}" FORWARD_IN="{forward}" SHIFT_IN="{shift}" INDEX_IN="{index}" BACK_L="{BACK_L}" BACK_M="{BACK_M}" BACK_S="{BACK_S}" FORWARD_L="{FORWARD_L}" FORWARD_M="{FORWARD_M}" FORWARD_S="{FORWARD_S}" SHIFT="{SHIFT}" DOCK_DIR="{DOCK_DIR}" />
        <Switch2 case_1="1" case_2="0" variable="{hasCompleteMaterial}">
          <Sequence>
            <ForceSuccess>
              <Timeout msec="6000">
                <FirmwareMission mission_status="{x}" mission_type="20" _description="back arm release 1 level" />
              </Timeout>
            </ForceSuccess>
            <SubTree ID="DockingSubTree" base="{PrevGoal}" dock_type="{DOCK_DIR}" final_pose="{PrevGoal}" isPureDocking="1" offset="{BACK_L}" retry="100" shift="0" sleep="100" timeout="6000" _autoremap="true" />
            <ForceSuccess>
              <Timeout msec="100">
                <FirmwareMission mission_status="{x}" mission_type="25" _description="close back side arm" />
              </Timeout>
            </ForceSuccess>
            <SetBlackboard value="0" output_key="hasCompleteMaterial" />
          </Sequence>
          <Sequence>
            <ForceSuccess>
              <Timeout msec="6000">
                <FirmwareMission mission_status="{x}" mission_type="24" _description="back arm release 1 level" />
              </Timeout>
            </ForceSuccess>
            <Sleep msec="200" />
            <SubTree ID="DockingSubTree" base="{PrevGoal}" dock_type="{DOCK_DIR}" final_pose="{PrevGoal}" isPureDocking="1" offset="{BACK_L}" retry="100" shift="0" sleep="100" timeout="6000" _autoremap="true" />
            <ForceSuccess>
              <Timeout msec="6000">
                <FirmwareMission mission_status="{x}" mission_type="23" _description="back arm release 1 level" />
              </Timeout>
            </ForceSuccess>
            <SetBlackboard value="0" output_key="hasCompleteMaterial" />
          </Sequence>
          <SubTree ID="DockingSubTree" base="{PrevGoal}" dock_type="{DOCK_DIR}" final_pose="{PrevGoal}" isPureDocking="1" offset="{BACK_L}" retry="100" shift="0" sleep="100" timeout="2000" _autoremap="true" />
        </Switch2>
        <Rotation final_pose="{PrevGoal}" degree="180" base="{PrevGoal}" />
        <SubTree ID="DockingSubTree" base="{PrevGoal}" dock_type="{DOCK_DIR}" final_pose="{PrevGoal}" isPureDocking="1" offset="{FORWARD_M}" retry="100" shift="{shift}" sleep="100" timeout="2700" _autoremap="true" />
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="35" _description="shrink little arm" />
          </Timeout>
        </ForceSuccess>
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="31" _description="arm to 1 level high" />
          </Timeout>
        </ForceSuccess>
        <SubTree ID="DockingSubTree" base="{PrevGoal}" dock_type="{DOCK_DIR}" final_pose="{PrevGoal}" isPureDocking="1" offset="{FORWARD_M}" retry="100" shift="0" sleep="100" timeout="2700" _autoremap="true" />
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="33" _description="arm down a little" />
          </Timeout>
        </ForceSuccess>
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="10" _description="front place 2 level" />
          </Timeout>
        </ForceSuccess>
        <SubTree ID="DockingSubTree" base="{PrevGoal}" dock_type="{DOCK_DIR}" final_pose="{PrevGoal}" isPureDocking="1" offset="{BACK_M}" retry="100" shift="0" sleep="100" timeout="6000" _autoremap="true" />
        <MissionSuccess levels="3" base_index="{index}" />
      </Sequence>
      <Sequence>
        <MySetBlackboard new_value="{last_mission_failed}" blackboard_value="true" blackboard_key="last_mission_failed" />
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="10" _description="smd strenght" />
          </Timeout>
        </ForceSuccess>
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="33" _description="smd strenght" />
          </Timeout>
        </ForceSuccess>
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="20" _description="smd strenght" />
          </Timeout>
        </ForceSuccess>
        <ForceSuccess>
          <MissionFailure failed_levels="{a}" success_levels="{b}" base_index="{index}" robot_type="1" step_results="5" />
        </ForceSuccess>
      </Sequence>
    </Fallback>
  </BehaviorTree>

  <BehaviorTree ID="TwoLevelsBot1new">
    <Fallback>
      <Sequence>
        <MissionStart BACK_IN="{back}" FORWARD_IN="{forward}" SHIFT_IN="{shift}" INDEX_IN="{index}" BACK_L="{BACK_L}" BACK_M="{BACK_M}" BACK_S="{BACK_S}" FORWARD_L="{FORWARD_L}" FORWARD_M="{FORWARD_M}" FORWARD_S="{FORWARD_S}" SHIFT="{SHIFT}" DOCK_DIR="{DOCK_DIR}" />
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="71" _description="take board" />
          </Timeout>
        </ForceSuccess>
        <SubTree ID="DockingSubTree" retry="100" timeout="6000" sleep="100" base="{PrevGoal}" offset="{BACK_S}" shift="0" isPureDocking="1" dock_type="{DOCK_DIR}" final_pose="{PrevGoal}" _autoremap="true" />
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="30" _description="arm down" />
          </Timeout>
        </ForceSuccess>
        <ForceSuccess>
          <SubTree ID="DockingSubTree" base="{PrevGoal}" dock_type="{DOCK_DIR}" final_pose="{PrevGoal}" isPureDocking="1" offset="{FORWARD_L}" retry="100" shift="0" sleep="100" timeout="1000" _autoremap="true" />
        </ForceSuccess>
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="15" _description="front take 1 level" />
          </Timeout>
        </ForceSuccess>
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="11" _description="front take 1 level" />
          </Timeout>
        </ForceSuccess>
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="32" _description="arm up" />
          </Timeout>
        </ForceSuccess>
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="70" _description="place board" />
          </Timeout>
        </ForceSuccess>
        <Parallel failure_count="1" success_count="2">
          <SubTree ID="DockingSubTree" base="{PrevGoal}" dock_type="{DOCK_DIR}" final_pose="{PrevGoal}" isPureDocking="0" offset="0" retry="100" shift="{SHIFT}" sleep="100" timeout="2700" _autoremap="true" />
          <ForceSuccess>
            <Timeout msec="6000">
              <FirmwareMission mission_status="{x}" mission_type="31" _description="arm to one level height" />
            </Timeout>
          </ForceSuccess>
        </Parallel>
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="33" _description="arm down a little" />
          </Timeout>
        </ForceSuccess>
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="10" _description="front place 1 level" />
          </Timeout>
        </ForceSuccess>
        <SubTree ID="DockingSubTree" base="{PrevGoal}" dock_type="{DOCK_DIR}" final_pose="{PrevGoal}" isPureDocking="1" offset="{BACK_M}" retry="100" shift="0" sleep="100" timeout="6000" _autoremap="true" />
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="30" _description="arm down" />
          </Timeout>
        </ForceSuccess>
        <ForceSuccess>
          <SubTree ID="DockingSubTree" base="{PrevGoal}" dock_type="{DOCK_DIR}" final_pose="{PrevGoal}" isPureDocking="1" offset="{FORWARD_M}" retry="100" shift="0" sleep="100" timeout="1000" _autoremap="true" />
        </ForceSuccess>
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="11" _description="front take 2 levels" />
          </Timeout>
        </ForceSuccess>
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="34" _description="smd strenght" />
          </Timeout>
        </ForceSuccess>
        <MissionSuccess levels="2" base_index="{index}" />
      </Sequence>
      <Sequence>
        <MySetBlackboard new_value="{last_mission_failed}" blackboard_value="true" blackboard_key="last_mission_failed" />
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="10" _description="smd strenght" />
          </Timeout>
        </ForceSuccess>
        <ForceSuccess>
          <Timeout msec="6000">
            <FirmwareMission mission_status="{x}" mission_type="33" _description="smd strenght" />
          </Timeout>
        </ForceSuccess>
      </Sequence>
      <ForceSuccess>
        <MissionFailure failed_levels="{a}" success_levels="{b}" robot_type="1" step_results="4" />
      </ForceSuccess>
    </Fallback>
  </BehaviorTree>

  <BehaviorTree ID="MissionBeforeTwoLevels">
    <Sequence>
      <ForceSuccess>
        <Timeout msec="6000">
          <FirmwareMission mission_status="{x}" mission_type="32" _description="take the raw material" />
        </Timeout>
      </ForceSuccess>
      <ForceSuccess>
        <Timeout msec="6000">
          <FirmwareMission mission_status="{x}" mission_type="12" _description="take the raw material" />
        </Timeout>
      </ForceSuccess>
      <ForceSuccess>
        <Timeout msec="6000">
          <FirmwareMission mission_status="{x}" mission_type="13" _description="take the raw material" />
        </Timeout>
      </ForceSuccess>
    </Sequence>
  </BehaviorTree>
  
  <TreeNodesModel>
    <Action ID="BTStarter">
      <output_port name="result" type="int" />
    </Action>
    <Action ID="BannerChecker">
      <output_port name="remap_banner_place" type="int" />
      <input_port name="banner_place" type="int" />
    </Action>
    <Action ID="CamReceiver" />
    <Action ID="Docking">
      <output_port name="final_pose" type="geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt;" />
      <input_port name="isPureDocking" type="bool" />
      <input_port name="dock_type" type="std::string" />
      <input_port name="shift" type="double" />
      <input_port name="offset" type="double" />
      <input_port name="base" type="geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt;" />
    </Action>
    <Action ID="GetLocation">
      <output_port name="pose" type="geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt;" />
      <input_port name="locaization_target" type="std::string" />
    </Action>
    <Action ID="Navigation">
        <output_port name="final_pose" type="geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt;"/>
        <input_port name="dock_type" type="std::string"/>
        <input_port name="goal" type="geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt;"/>
    </Action>
    <Action ID="FirmwareMission">
      <output_port name="mission_status" type="int" />
      <input_port name="mission_type" type="int" />
    </Action>
    <Decorator ID="MaterialChecker">
      <output_port name="remap_offset" type="double" />
      <output_port name="remap_shift" type="double" />
      <output_port name="remap_dock_type" type="std::string" />
      <output_port name="remap_base" type="geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt;" />
      <output_port name="remap_index" type="int" />
      <input_port name="shift" type="double" />
      <input_port name="offset" type="double" />
      <input_port name="mission_type" type="std::string" />
      <input_port name="dock_type" type="std::string" />
      <input_port name="base_index" type="int" />
    </Decorator>
    <Decorator ID="MissionChecker">
      <output_port name="level" type="int" />
      <output_port name="remap_base" type="geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt;" />
      <input_port name="mission_type" type="std::string" />
      <input_port name="base_index" type="int" />
    </Decorator>
    <Action ID="MissionFailure">
      <output_port name="failed_levels" type="int" />
      <output_port name="success_levels" type="int" />
      <input_port name="robot_type" type="bool" />
      <input_port name="step_results" type="std::deque&lt;int, std::allocator&lt;int&gt; &gt;" />
    </Action>
    <Action ID="MissionStart" editable="true">
      <input_port name="BACK_IN" />
      <input_port name="FORWARD_IN" />
      <input_port name="SHIFT_IN" />
      <input_port name="INDEX_IN" />
      <output_port name="BACK_L" />
      <output_port name="BACK_M" />
      <output_port name="BACK_S" />
      <output_port name="FORWARD_L" />
