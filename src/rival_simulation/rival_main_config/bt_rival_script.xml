<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <initPoints material_points="{Materials}"
                  garbage_points="{OBS}"/>
      <Navigation final_pose="{PrevGoal}"
                  goal="0.8, 1.7, 0"
                  start_pose="0.4, 1.7, 0"/>
      <StateUpdater garbages_out="{OBS}"
                    garbages_in="{OBS}"
                    materials_out="{Materials}"
                    materials_in="{Materials}"
                    new_garbage_position="-1, -1, -1"
                    changed_point_code="9"
                    point_status="0"
                    changed_point_position="0.8, 1.7, 0"/>
      <Navigation final_pose="{PrevGoal}"
                  goal="0.3, 1.3, 0"
                  start_pose="{PrevGoal}"/>
      <StateUpdater garbages_out="{OBS}"
                    garbages_in="{OBS}"
                    materials_out="{Materials}"
                    materials_in="{Materials}"
                    new_garbage_position="-1, -1, -1"
                    changed_point_code="7"
                    point_status="1"
                    changed_point_position="0.3, 1.3, 0"/>
      <Navigation final_pose="{PrevGoal}"
                  goal="0.8, 0.25, 0"
                  start_pose="{PrevGoal}"/>
      <Navigation final_pose="{PrevGoal}"
                  goal="1.1, 0.8, 0"
                  start_pose="{PrevGoal}"/>
      <StateUpdater garbages_out="{OBS}"
                    garbages_in="{OBS}"
                    materials_out="{Materials}"
                    materials_in="{Materials}"
                    new_garbage_position="-1, -1, -1"
                    changed_point_code="5"
                    point_status="0"
                    changed_point_position="1.1, 0.8, 0"/>
      <Navigation final_pose="{PrevGoal}"
                  goal="0.3, 0.4, 0"
                  start_pose="{PrevGoal}"/>
      <StateUpdater garbages_out="{OBS}"
                    garbages_in="{OBS}"
                    materials_out="{Materials}"
                    materials_in="{Materials}"
                    new_garbage_position="-1, -1, -1"
                    changed_point_code="8"
                    point_status="1"
                    changed_point_position="0.3, 0.4, 0"/>
      <Navigation final_pose="{PrevGoal}"
                  goal="1.25, 0.5, 0"
                  start_pose="{PrevGoal}"/>
      <Navigation final_pose="{PrevGoal}"
                  goal="0.4, 1.7, 0"
                  start_pose="{PrevGoal}"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="NavTest">
    <Sequence>
      <Navigation final_pose="{PrevGoal}"
                  goal="0.8, 1.7, 0"
                  start_pose="0.4, 1.7, 0"/>
      <Navigation final_pose="{PrevGoal}"
                  goal="0.3, 1.3, 0"
                  start_pose="{PrevGoal}"/>
    </Sequence>
  </BehaviorTree>

  <!-- Description of Node Models (used by Groot) -->
  <TreeNodesModel>
    <Action ID="Navigation">
      <output_port name="final_pose"
                   type="geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt;"/>
      <input_port name="goal"
                  type="geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt;"/>
      <input_port name="start_pose"
                  type="geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt;"/>
    </Action>
    <Action ID="StateUpdater">
      <output_port name="garbages_out"
                   type="std::shared_ptr&lt;std::deque&lt;geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt;, std::allocator&lt;geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt; &gt; &gt; &gt;"/>
      <input_port name="garbages_in"
                  type="std::shared_ptr&lt;std::deque&lt;geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt;, std::allocator&lt;geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt; &gt; &gt; &gt;"/>
      <output_port name="materials_out"
                   type="std::shared_ptr&lt;std::deque&lt;geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt;, std::allocator&lt;geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt; &gt; &gt; &gt;"/>
      <input_port name="materials_in"
                  type="std::shared_ptr&lt;std::deque&lt;geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt;, std::allocator&lt;geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt; &gt; &gt; &gt;"/>
      <input_port name="new_garbage_position"
                  type="geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt;"/>
      <input_port name="changed_point_code"
                  type="int"/>
      <input_port name="point_status"
                  type="int"/>
      <input_port name="changed_point_position"
                  type="geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt;"/>
    </Action>
    <Action ID="initPoints">
      <output_port name="material_points"
                   type="std::shared_ptr&lt;std::deque&lt;geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt;, std::allocator&lt;geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt; &gt; &gt; &gt;"/>
      <output_port name="garbage_points"
                   type="std::shared_ptr&lt;std::deque&lt;geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt;, std::allocator&lt;geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt; &gt; &gt; &gt;"/>
    </Action>
  </TreeNodesModel>

</root>
