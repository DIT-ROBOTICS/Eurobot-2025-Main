<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4">
  <BehaviorTree ID="EasyMainTree">
    <Sequence>
      <PublishPose/>
      <Navigation final_pose="{PrevGoal}"
                  goal_index="11"
                  goal=""
                  start_pose="2.65, 0.9, 2"/>
      <LoopDouble if_empty="SUCCESS"
                  value="{index}"
                  queue="1, 2, 11, 3, 11, 4, 6, 13, 10">
        <Sequence>
          <Navigation final_pose="{PrevGoal}"
                      goal_index="{index}"
                      goal=""
                      start_pose="{PrevGoal}"/>
          <StateUpdater changed_point_code="{index}"/>
          <Sleep msec="5000"/>
        </Sequence>
      </LoopDouble>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="MainTree">
    <Sequence>
      <PublishPose/>
      <Sequence>
        <Navigation final_pose="{PrevGoal}"
                    goal_index=""
                    goal="0.8, 1.7, 0"
                    start_pose="0.4, 1.7, 0"/>
        <StateUpdater changed_point_code="9"/>
        <Sleep msec="5000"/>
      </Sequence>
      <Sequence>
        <Navigation final_pose="{PrevGoal}"
                    goal_index=""
                    goal="0.3, 1.3, 0"
                    start_pose="{PrevGoal}"/>
        <StateUpdater changed_point_code="8"/>
        <Sleep msec="5000"/>
      </Sequence>
      <Sequence>
        <Navigation final_pose="{PrevGoal}"
                    goal_index=""
                    goal="0.8, 0.25, 0"
                    start_pose="{PrevGoal}"/>
        <Sleep msec="15000"/>
      </Sequence>
      <Sequence>
        <Navigation final_pose="{PrevGoal}"
                    goal_index=""
                    goal="1.1, 0.8, 0"
                    start_pose="{PrevGoal}"/>
        <StateUpdater changed_point_code="6"/>
        <Sleep msec="5000"/>
      </Sequence>
      <Sequence>
        <Navigation final_pose="{PrevGoal}"
                    goal_index=""
                    goal="0.3, 0.4, 0"
                    start_pose="{PrevGoal}"/>
        <StateUpdater changed_point_code="7"/>
        <Sleep msec="5000"/>
      </Sequence>
      <Sequence>
        <Navigation final_pose="{PrevGoal}"
                    goal_index=""
                    goal="1.25, 0.5, 0"
                    start_pose="{PrevGoal}"/>
        <Sleep msec="15000"/>
      </Sequence>
      <Navigation final_pose="{PrevGoal}"
                  goal_index=""
                  goal="0.4, 1.7, 0"
                  start_pose="{PrevGoal}"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="NavTest">
    <Sequence>
      <Navigation final_pose="{PrevGoal}"
                  goal_index=""
                  goal="0.8, 1.7, 0"
                  start_pose="0.4, 1.7, 0"/>
      <Navigation final_pose="{PrevGoal}"
                  goal_index=""
                  goal="0.8, 1.7, 0"
                  start_pose="0.4, 1.7, 0"/>
      <Navigation final_pose="{PrevGoal}"
                  goal_index=""
                  goal="0.3, 1.3, 0"
                  start_pose="{PrevGoal}"/>
      <Navigation final_pose="{PrevGoal}"
                  goal_index=""
                  goal="0.8, 0.25, 0"
                  start_pose="{PrevGoal}"/>
    </Sequence>
  </BehaviorTree>

  <!-- Description of Node Models (used by Groot) -->
  <TreeNodesModel>
    <Action ID="Navigation">
      <output_port name="final_pose"
                   type="geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt;"/>
      <input_port name="goal_index"
                  type="int"/>
      <input_port name="goal"
                  type="geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt;"/>
      <input_port name="start_pose"
                  type="geometry_msgs::msg::PoseStamped_&lt;std::allocator&lt;void&gt; &gt;"/>
    </Action>
    <Action ID="PublishPose"/>
    <Action ID="StateUpdater">
      <input_port name="changed_point_code"
                  type="int"/>
    </Action>
  </TreeNodesModel>

</root>
